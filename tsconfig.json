{

    "include": ["src"],//TS 파일이 어디 위치하는지
    "compilerOptions": { 
        "outDir":"build",//TS 컴파일한 JS 파일 어디에 만들지
        "target": "ES6",// 어떤 JS버전으로 컴파일 할건지
        "lib":["ES6","DOM"],// TS 가 어떤 환경에서 작동할 것인지
        "strict":true, // strict 모드 활성화
        "allowJs":true,
        //"esModuleInterop":true,
        //"module":"CommonJS" // 브라우저 앱이라면 "umd" 

    }
    
}